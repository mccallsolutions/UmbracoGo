@using ClientDependency.Core.Mvc
@using UmbracoGo.Web
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@Html.Action(MVC.WebPage.MetaTitle())</title>
    @Html.Action(MVC.WebPage.MetaElements())
    @{
        Html.RequiresCss("~/Content/bootstrap.min.css");
        Html.RequiresCss("~/Content/Site.css");
    }
    @Html.RenderCssHere()
    @Html.RenderJsHere(ApplicationConstants.HeadRendererProvider)
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Application name", MVC.HomeLandingPage.Index(), new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                </ul>
            </div>
        </div>
    </div>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - @Html.Action(MVC.Website.SiteName())</p>
        </footer>
    </div>    
    @{
        Html.RequiresJs("~/Scripts/jquery-2.1.4.min.js");
        Html.RequiresJs("~/Scripts/bootstrap.min.js");
    }
    @Html.RenderJsHere()    
</body>
</html>